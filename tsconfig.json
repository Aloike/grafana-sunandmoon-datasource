{
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "ES5",
    "sourceMap": true,
    "declaration": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "outDir": "dist/"
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
